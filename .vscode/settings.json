{
  "editor.fontFamily": "\"Dank Mono\", \"Fira Code\"",
  "editor.fontLigatures": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": ["markup.quote.markdown"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["markup.italic.markdown", "punctuation.definition.italic.markdown"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["markup.inline.raw.string.markdown", "markup.fenced_code.block.markdown"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Comment",
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "String Quoted",
        "scope": ["string.quoted", "variable.other.readwrite.js"],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Variable",
        "scope": "variable",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "JavaScript Other Variable",
        "scope": "variable.other.object.js",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Variable Property Other object property",
        "scope": ["variable.other.object.property"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Storage",
        "scope": "storage",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Keywords and Storage types",
        "scope": ["keyword", "storage.type", "storage.modifier", "variable.language.this"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Keywords operators",
        "scope": ["keyword.operator"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Storage",
        "scope": [
          "storage",
          "meta.var.expr",
          "meta.class meta.method.declaration meta.var.expr storage.type.js",
          "storage.type.property.js",
          "storage.type.property.ts"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Inherited class",
        "scope": "entity.other.inherited-class",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Variables, Let and Const",
        "scope": ["variable.other.readwrites", "meta.definition.variable"],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Function name",
        "scope": "entity.name.function",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Function argument",
        "scope": "variable.parameter",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Tag name",
        "scope": "entity.name.tag",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Tag attribute",
        "scope": "entity.other.attribute-name",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Meta - Decorator",
        "scope": ["punctuation.decorator"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Library function",
        "scope": "support.function",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Library constant",
        "scope": "support.constant",
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Invalid",
        "scope": "invalid",
        "settings": {
          "fontStyle": " italic bold underline"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": "invalid.deprecated",
        "settings": {
          "fontStyle": " bold italic underline"
        }
      },
      {
        "name": "Headings",
        "scope": ["markup.heading punctuation.definition.heading", "entity.name.section"],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "Quotes",
        "scope": "markup.quote",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "typescriptHero.imports.removeTrailingIndex": false,
  "typescriptHero.imports.grouping": ["/@angular/", "/@ngrx/", "/rxjs/", "/@rds", "Modules", "Plains", "Workspace"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "typescriptHero.imports.organizeOnSave": true
}
